{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "pretttier/@typescript-eslint",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommend"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":"error",
        "import/extensions":[
            "error",
            "ignorePackages",
            {"ts":"never"}
        ]
    },
    "settings": {
        "import/resolver":{
            "typescript":{

            }
        }
    }
}
