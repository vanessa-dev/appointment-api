{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "pretttier/@typescript-eslint",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommend"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "no-underscore-dangle":"off",
        "prettier/prettier":"error",
        "class-methods-use-this":"off",
        "@typescript-eslint/camelcase":"off",
        "@typescript-eslint/no-unused-vars":["error", {
            "argsIgnorePattern":"_"
        }],
        "import/extensions":[
            "error",
            "ignorePackages",
            {"ts":"never"}
        ]
    },
    "settings": {
        "import/resolver":{
            "typescript":{

            }
        }
    }
}
